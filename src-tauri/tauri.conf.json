{
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "keyway",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "identifier": "keyway.app",
      "targets": "all",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
        {
            "label": "ConfigureWindow",
            "title": "Keyway Configure",
            "width": 400,
            "height": 800,
            "url": "config.html"
        },
        {
            "label": "KeyWindow",
            "title": "Keyway Display",
            "width": 800,
            "height": 200,
            "url": "key.html"
        }
    ]
  }
}
